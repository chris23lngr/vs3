{
	"name": "vs3-examples-vue",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "nuxt build",
		"check": "biome check .",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"dev": "nuxt dev --port 3010",
		"preview": "nuxt preview",
		"typecheck": "nuxt prepare && tsc --noEmit -p .nuxt/tsconfig.json"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.981.0",
		"nuxt": "^3.19.3",
		"vs3": "workspace:*",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"typescript": "^5.9.3"
	},
	"packageManager": "pnpm@10.28.1"
}
