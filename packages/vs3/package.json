{
	"name": "vs3",
	"version": "0.1.0-beta.1",
	"description": "Type-safe S3 storage library with presigned uploads, validation, and framework integrations",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./react": {
			"import": {
				"types": "./dist/react.d.ts",
				"default": "./dist/react.js"
			},
			"require": {
				"types": "./dist/react.d.cts",
				"default": "./dist/react.cjs"
			}
		},
		"./integrations/next-js": {
			"import": {
				"types": "./dist/next-js.d.ts",
				"default": "./dist/next-js.js"
			},
			"require": {
				"types": "./dist/next-js.d.cts",
				"default": "./dist/next-js.cjs"
			}
		},
		"./adapters": {
			"import": {
				"types": "./dist/adapters.d.ts",
				"default": "./dist/adapters.js"
			},
			"require": {
				"types": "./dist/adapters.d.cts",
				"default": "./dist/adapters.cjs"
			}
		},
		"./package.json": "./package.json",
		"./middleware/auth": {
			"import": {
				"types": "./dist/middleware-auth.d.ts",
				"default": "./dist/middleware-auth.js"
			},
			"require": {
				"types": "./dist/middleware-auth.d.cts",
				"default": "./dist/middleware-auth.cjs"
			}
		}
	},
	"files": [
		"dist"
	],
	"sideEffects": false,
	"typesVersions": {
		"*": {
			"react": [
				"./dist/react.d.ts"
			],
			"integrations/next-js": [
				"./dist/next-js.d.ts"
			],
			"adapters": [
				"./dist/adapters.d.ts"
			],
			"middleware/auth": [
				"./dist/middleware-auth.d.ts"
			]
		}
	},
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"lint": "biome check .",
		"format": "biome check --write .",
		"format:unsafe": "biome check --write --unsafe .",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"prepublishOnly": "tsup"
	},
	"keywords": [
		"s3",
		"storage",
		"upload",
		"aws",
		"presigned-url",
		"file-upload",
		"typescript"
	],
	"author": "chris23lngr",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/chris23lngr/vs3.git",
		"directory": "packages/vs3"
	},
	"bugs": {
		"url": "https://github.com/chris23lngr/vs3/issues"
	},
	"homepage": "https://github.com/chris23lngr/vs3#readme",
	"devDependencies": {
		"@aws-sdk/client-s3": "^3.981.0",
		"@types/node": "^25.2.0",
		"@types/react": "^18.3.27",
		"@vitest/coverage-v8": "4.0.18",
		"@vitest/ui": "^4.0.18",
		"better-auth": "^1.4.18",
		"react": "^19.2.4",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"@aws-sdk/s3-request-presigner": "^3.981.0",
		"@nanostores/react": "^1.0.0",
		"better-call": "^1.2.1",
		"better-fetch": "^1.1.2",
		"nanostores": "^1.1.0",
		"zod": "^4.3.6"
	},
	"peerDependencies": {
		"@aws-sdk/client-s3": "^3.981.0",
		"react": ">=18"
	},
	"peerDependenciesMeta": {
		"react": {
			"optional": true
		}
	}
}
