{
	"$schema": "https://turborepo.dev/schema.v2.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"test": {
			"dependsOn": ["build"]
		},
		"typecheck": {
			"dependsOn": ["^build"]
		},
		"lint": {},
		"format": {},
		"format:unsafe": {}
	}
}
